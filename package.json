{
  "name":"genkit",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "format:check": "npm-run-all format:check-genkit format:check-genkit-tools",
    "format:check-genkit": "cd genkit && npm run format:check",
    "format:check-genkit-tools": "cd genkit-tools && npm run format:check",
    "format": "npm-run-all format:genkit format:genkit-tools",
    "format:genkit": "cd genkit && npm run format",
    "format:genkit-tools": "cd genkit-tools && npm run format",
    "build:all": "npm-run-all build:genkit build:genkit-tools",
    "build:genkit": "cd genkit && npm i && npm run build:all",
    "build:genkit-tools": "cd genkit-tools && npm i && npm run build",
    "npm-install-genkit": "cd genkit && npm install",
    "npm-install-genkit-tools": "cd genkit-tools && npm install",
    "link-genkit-tools": "cd genkit-tools && npm link",
    "setup": "npm-run-all npm-install-genkit npm-install-genkit-tools build:all link-genkit-tools",
    "pack:all": "rm -r dist || true && mkdir dist && npm-run-all pack:genkit pack:tools dist:zip",
    "pack:tools": "cd genkit-tools && npm run pack:all",
    "pack:genkit": "cd genkit && npm run pack:all",
    "dist:zip": "cd dist && zip genkit-dist.zip *.tgz"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
