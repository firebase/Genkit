{
  "name": "@google-genkit/plugin-ragas",
  "version": "0.0.2",
  "type": "commonjs",
  "scripts": {
    "check": "tsc",
    "compile": "tsup-node",
    "build:clean": "rm -rf ./lib",
    "build": "npm-run-all build:clean check compile",
    "build:watch": "tsup-node --watch",
    "lint": "eslint --config ../../.eslintrc.js --ext .ts src",
    "lint:fix": "npm run lint -- --fix --quiet"
  },
  "dependencies": {
    "@google-genkit/ai": "*",
    "@google-genkit/common": "*",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "npm-run-all": "^4.1.5",
    "tsup": "^8.0.2",
    "tsx": "^4.7.0",
    "typescript": "^4.9.0"
  },
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./lib/index.mjs",
      "types": "./lib/index.d.ts"
    }
  }
}
