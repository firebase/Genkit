{
  "name": "@genkit-ai/tools-plugins",
  "version": "0.5.0-rc.5",
  "scripts": {
    "compile": "tsc -b ./tsconfig.cjs.json ./tsconfig.esm.json ./tsconfig.types.json",
    "build:clean": "rm -rf ./lib",
    "build": "npm-run-all build:clean compile",
    "build:watch": "tsc -b ./tsconfig.cjs.json ./tsconfig.esm.json ./tsconfig.types.json --watch"
  },
  "dependencies": {
    "colorette": "^2.0.20",
    "zod": "^3.23.3"
  },
  "devDependencies": {
    "@types/node": "^20.11.17",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.9.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/google/genkit.git",
    "directory": "genkit-tools/plugins/common"
  },
  "author": "genkit",
  "license": "Apache-2.0",
  "types": "lib/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/types/index.d.ts",
      "require": "./lib/cjs/index.js",
      "import": "./lib/esm/index.js",
      "default": "./lib/esm/index.js"
    },
    "./config": {
      "types": "./lib/types/config.d.ts",
      "require": "./lib/cjs/config.js",
      "import": "./lib/esm/config.js",
      "default": "./lib/esm/config.js"
    },
    "./plugins": {
      "types": "./lib/types/plugins.d.ts",
      "require": "./lib/cjs/plugins.js",
      "import": "./lib/esm/plugins.js",
      "default": "./lib/esm/plugins.js"
    }
  },
  "typesVersions": {
    "*": {
      "config": [
        "lib/types/config"
      ],
      "plugins": [
        "lib/types/plugins"
      ]
    }
  }
}
