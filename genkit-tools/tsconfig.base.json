{
  "compileOnSave": false,
  "compilerOptions": {
    "esModuleInterop": true,
    "strict": true,
    "strictNullChecks": true,
    "removeComments": true,
    "target": "ES2022",
    "sourceMap": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": false,
    "skipLibCheck": true,
    "declaration": true,
    "declarationMap": true
  },
  "lib": ["ES2022"]
}
