{
  "name": "genkit-cli",
  "version": "0.0.5",
  "description": "CLI for interacting with the Google GenKit AI framework",
  "bin": {
    "genkit": "dist/bin/genkit.js"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "pnpm install && pnpm build:common && pnpm build:cli && pnpm build:ui && pnpm copy-ui-build && pnpm build:plugins",
    "build:cli": "tsc",
    "build:cli:watch": "tsc --watch",
    "build:ui": "cd ui && pnpm build",
    "build:common": "cd common && pnpm build",
    "build:plugins": "(cd plugins/google && pnpm build) && (cd plugins/firebase && pnpm build)",
    "copy-ui-build": "rm -rf dist/client && cd ui && rsync -r dist ../dist/client",
    "test:all": "pnpm run test:ui && pnpm run test:jest",
    "test:ui": "cd ui && pnpm test-headless",
    "test:jest": "jest --verbose",
    "export:schemas": "ts-node scripts/schema-exporter.ts .",
    "pack:all": "pnpm run pack:cli && pnpm run pack:common && pnpm run pack:plugins",
    "pack:cli": "pnpm pack --pack-destination ../dist",
    "pack:common": "cd common && pnpm pack --pack-destination ../../dist",
    "pack:plugins": "for i in plugins/*/; do cd $i && pnpm pack --pack-destination ../../../dist && cd ../..; done"
  },
  "author": "Firebase (https://firebase.google.com)",
  "license": "MIT",
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^6.3.1",
    "@genkit-ai/tools-plugins": "workspace:*",
    "@trpc/server": "10.45.0",
    "axios": "^1.6.7",
    "body-parser": "^1.20.2",
    "chokidar": "^3.5.3",
    "colorette": "^2.0.20",
    "commander": "^11.1.0",
    "configstore": "^5.0.1",
    "express": "^4.18.2",
    "extract-zip": "^2.0.1",
    "firebase-admin": "^12.0.0",
    "inquirer": "^8.2.0",
    "js-yaml": "^4.1.0",
    "json-schema": "^0.4.0",
    "open": "^6.3.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.22.4"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/body-parser": "^1.19.5",
    "@types/cli-color": "^2.0.6",
    "@types/configstore": "^6.0.2",
    "@types/express": "^4.17.21",
    "@types/inquirer": "^8.1.3",
    "@types/jest": "^29.5.12",
    "@types/js-yaml": "^4.0.9",
    "@types/json-schema": "^7.0.15",
    "@types/node": "^20.11.19",
    "@types/uuid": "^9.0.8",
    "jest": "^29.7.0",
    "memfs": "^4.7.7",
    "npm-run-all": "^4.1.5",
    "only-allow": "^1.2.1",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "packageManager": "pnpm@8.15.5+sha256.4b4efa12490e5055d59b9b9fc9438b7d581a6b7af3b5675eb5c5f447cee1a589"
}
