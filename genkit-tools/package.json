{
  "name": "genkit-cli",
  "version": "0.0.2",
  "description": "CLI for interacting with the Google GenKit AI framework",
  "bin": {
    "genkit": "dist/bin/genkit.js"
  },
  "workspaces": [
    "./*",
    "./common/*",
    "./plugins/*",
    "./ui/*"
  ],
  "main": "dist/index.js",
  "scripts": {
    "build": "(npm run init:deps) && (npm run build:common) && (npm run build:cli) && (npm run build:ui) && (npm run copy-ui-build) && (npm run build:plugins)",
    "build:cli": "tsc",
    "build:ui": "cd ui && npm run build",
    "build:common": "cd common && npm run build",
    "build:plugins": "(cd plugins/google && npm run build) && (cd plugins/firebase && npm run build)",
    "copy-ui-build": "rm -rf dist/client && cd ui && rsync -r dist ../dist/client",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "test": "echo \"Error: no test specified\" && exit 1",
    "init:deps": "(npm install --ignore-scripts -C ./ui); (npm install --ignore-scripts -C ./common)",
    "pack:all": "npm-run-all pack:cli pack:common pack:plugins",
    "pack:cli": "npm pack --pack-destination ../dist",
    "pack:common": "cd common && npm pack --pack-destination ../../dist -ws",
    "pack:plugins": "for i in plugins/*/; do cd $i && npm pack --pack-destination ../../../dist -ws && cd ../..; done"
  },
  "author": "Firebase (https://firebase.google.com)",
  "license": "MIT",
  "devDependencies": {
    "@types/cli-color": "^2.0.6",
    "@types/express": "^4.17.21",
    "@types/js-yaml": "^4.0.9",
    "@types/json-schema": "^7.0.15",
    "@types/node": "^20.11.19",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^6.3.1",
    "@trpc/server": "10.45.0",
    "axios": "^1.6.7",
    "chokidar": "^3.5.3",
    "colorette": "^2.0.20",
    "commander": "^11.1.0",
    "express": "^4.18.2",
    "firebase-admin": "^12.0.0",
    "js-yaml": "^4.1.0",
    "json-schema": "^0.4.0",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  }
}
