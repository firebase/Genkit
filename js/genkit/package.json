      "import": "./lib/ai/retriever.mjs",
      "default": "./lib/ai/retriever.js"
    },
    "./ai/embedder": {
      "types": "./lib/ai/embedder.d.ts",
      "require": "./lib/ai/embedder.js",
      "import": "./lib/ai/embedder.mjs",
      "default": "./lib/ai/embedder.js"
    },
    "./ai/evaluator": {
      "types": "./lib/ai/evaluator.d.ts",
      "require": "./lib/ai/evaluator.js",
      "import": "./lib/ai/evaluator.mjs",
      "default": "./lib/ai/evaluator.js"
    },
    "./ai/model": {
      "types": "./lib/ai/model.d.ts",
