      "types": "./lib/ai/tool.d.ts",
      "require": "./lib/ai/tool.js",
      "import": "./lib/ai/tool.mjs",
      "default": "./lib/ai/tool.js"
    },
    "./flow": {
      "types": "./lib/flow/index.d.ts",
      "require": "./lib/flow/index.js",
      "import": "./lib/flow/index.mjs",
      "default": "./lib/flow/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "metrics": [
        "lib/metrics"
      ],
      "registry": [
        "lib/registry"
      ],
      "tracing": [
