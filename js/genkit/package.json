      ],
      "logging": [
        "lib/logging"
      ],
      "config": [
        "lib/config"
      ],
      "runtime": [
        "lib/runtime"
      ],
      "schema": [
        "lib/schema"
      ],
      "ai": [
        "lib/ai"
      ],
      "ai/retriever": [
        "lib/ai/retriever"
      ],
      "ai/embedder": [
        "lib/ai/embedder"
      ],
      "ai/evaluator": [
        "lib/ai/evaluator"
      ],
      "ai/model": [
        "lib/ai/model"
      ],
      "ai/model/middleware": [
        "lib/ai/middleware"
      ],
      "ai/extract": [
        "lib/ai/extract"
      ],
      "ai/tool": [
        "lib/ai/tool"
      ],
      "flow": [
        "lib/flow"
      ]
    }
  }
}
