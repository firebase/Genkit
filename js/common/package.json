{
  "name": "@genkit-ai/common",
  "version": "0.0.5",
  "type": "commonjs",
  "scripts": {
    "check": "tsc",
    "compile": "tsup-node",
    "build:clean": "rm -rf ./lib",
    "build": "npm-run-all build:clean check compile",
    "build:watch": "tsup-node --watch",
    "lint": "eslint --config ../.eslintrc.js --ext .ts src",
    "lint:fix": "npm run lint -- --fix --quiet",
    "test": "node --import tsx --test tests/*_test.ts"
  },
  "dependencies": {
    "@google-cloud/monitoring": "^4.0.0",
    "@google-cloud/opentelemetry-cloud-monitoring-exporter": "^0.17.0",
    "@google-cloud/trace-agent": "^7.1.2",
    "@opentelemetry/api": "^1.7.0",
    "@opentelemetry/context-async-hooks": "^1.20.0",
    "@opentelemetry/sdk-metrics": "^1.22.0",
    "@opentelemetry/sdk-node": "^0.49.0",
    "@opentelemetry/sdk-trace-base": "^1.22.0",
    "express": "^4.18.2",
    "express-openapi-validator": "^5.1.3",
    "firebase-admin": "^11.11.0",
    "json-schema": "^0.4.0",
    "winston": "^3.12.0",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "npm-run-all": "^4.1.5",
    "tsup": "^8.0.2",
    "tsx": "^4.7.0",
    "typescript": "^4.9.0"
  },
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "require": "./lib/index.js",
      "import": "./lib/index.mjs"
    },
    "./metrics": {
      "types": "./lib/metrics.d.ts",
      "require": "./lib/metrics.js",
      "import": "./lib/metrics.mjs"
    },
    "./registry": {
      "types": "./lib/registry.d.ts",
      "require": "./lib/registry.js",
      "import": "./lib/registry.mjs"
    },
    "./tracing": {
      "types": "./lib/tracing.d.ts",
      "require": "./lib/tracing.js",
      "import": "./lib/tracing.mjs"
    },
    "./logging": {
      "types": "./lib/logging.d.ts",
      "require": "./lib/logging.js",
      "import": "./lib/logging.mjs"
    },
    "./config": {
      "types": "./lib/config.d.ts",
      "require": "./lib/config.js",
      "import": "./lib/config.mjs"
    },
    "./runtime": {
      "types": "./lib/runtime.d.ts",
      "require": "./lib/runtime.js",
      "import": "./lib/runtime.mjs"
    }
  },
  "typesVersions": {
    "*": {
      "metrics": [
        "lib/metrics"
      ],
      "registry": [
        "lib/registry"
      ],
      "tracing": [
        "lib/tracing"
      ],
      "logging": [
        "lib/logging"
      ],
      "config": [
        "lib/config"
      ],
      "runtime": [
        "lib/runtime"
      ]
    }
  }
}
