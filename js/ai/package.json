{
  "name": "@genkit-ai/ai",
  "version": "0.0.4",
  "type": "commonjs",
  "scripts": {
    "check": "tsc",
    "compile": "tsup-node",
    "build:clean": "rm -rf ./lib",
    "build": "npm-run-all build:clean check compile",
    "build:watch": "tsup-node --watch",
    "lint": "eslint --config ../.eslintrc.js --ext .ts src",
    "lint:fix": "npm run lint -- --fix --quiet",
    "test": "node --import tsx --test ./tests/**/*_test.ts"
  },
  "dependencies": {
    "@genkit-ai/common": "*",
    "@types/node": "^20.11.19",
    "crypto": "^1.0.1",
    "front-matter": "^4.0.2",
    "node-fetch": "^3.3.2",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.21.4"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "tsx": "^4.7.1",
    "typescript": "^4.9.0"
  },
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "require": "./lib/index.js",
      "import": "./lib/index.mjs"
    },
    "./retrievers": {
      "types": "./lib/retrievers.d.ts",
      "require": "./lib/retrievers.js",
      "import": "./lib/retrievers.mjs"
    },
    "./embedders": {
      "types": "./lib/embedders.d.ts",
      "require": "./lib/embedders.js",
      "import": "./lib/embedders.mjs"
    },
    "./evaluators": {
      "types": "./lib/evaluators.d.ts",
      "require": "./lib/evaluators.js",
      "import": "./lib/evaluators.mjs"
    },
    "./model": {
      "types": "./lib/model.d.ts",
      "require": "./lib/model.js",
      "import": "./lib/model.mjs"
    },
    "./model/middleware": {
      "types": "./lib/model/middleware.d.ts",
      "require": "./lib/model/middleware.js",
      "import": "./lib/model/middleware.mjs"
    },
    "./generate": {
      "types": "./lib/generate.d.ts",
      "require": "./lib/generate.js",
      "import": "./lib/generate.mjs"
    },
    "./extract": {
      "types": "./lib/extract.d.ts",
      "require": "./lib/extract.js",
      "import": "./lib/extract.mjs"
    },
    "./tool": {
      "types": "./lib/tool.d.ts",
      "require": "./lib/tool.js",
      "import": "./lib/tool.mjs"
    }
  },
  "typesVersions": {
    "*": {
      "retrievers": [
        "lib/retrievers"
      ],
      "embedders": [
        "lib/embedders"
      ],
      "evaluators": [
        "lib/evaluators"
      ],
      "model": [
        "lib/model"
      ],
      "model/middleware": [
        "lib/model/middleware"
      ],
      "generate": [
        "lib/generate"
      ],
      "extract": [
        "lib/extract"
      ],
      "tool": [
        "lib/tool"
      ]
    }
  }
}
