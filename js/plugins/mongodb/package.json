{
  "name": "genkitx-mongodb",
  "description": "Genkit AI framework plugin for MongoDB vector database.",
  "keywords": [
    "genkit",
    "genkit-retriever",
    "genkit-plugin",
    "genkit-indexer",
    "mongodb",
    "vector",
    "vector-search",
    "text-search",
    "hybrid-search",
    "atlas-search"
  ],
  "version": "1.0.0",
  "type": "commonjs",
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./lib/index.mjs",
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    }
  },
  "scripts": {
    "check": "tsc",
    "compile": "tsup-node",
    "build:clean": "rimraf ./lib",
    "build": "npm-run-all build:clean check compile",
    "build:watch": "tsup-node --watch",
    "test": "jest --verbose",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/firebase/genkit.git",
    "directory": "js/plugins/mongodb"
  },
  "author": "genkit",
  "license": "Apache-2.0",
  "peerDependencies": {
    "genkit": "workspace:^"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "rimraf": "^6.0.1",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "mongodb": "^6.17.0"
  }
}
