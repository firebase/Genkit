{
  "name": "genkitx-graph",
  "description": "Genkit AI framework plugin for graphs.",
  "keywords": [
    "genkit",
    "genkit-plugin",
    "genkit-deploy",
    "genkit-flow",
    "ai",
    "genai",
    "generative-ai"
  ],
  "version": "0.5.7",
  "type": "commonjs",
  "scripts": {
    "check": "tsc",
    "compile": "tsup-node",
    "build:clean": "rm -rf ./lib",
    "build": "npm-run-all build:clean check compile",
    "build:watch": "tsup-node --watch",
    "test": "node --import tsx --test tests/*_test.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/firebase/genkit.git",
    "directory": "js/plugins/firebase"
  },
  "author": "genkit",
  "license": "Apache-2.0",
  "dependencies": {
    "@genkit-ai/ai": "^0.5.7",
    "@genkit-ai/core": "^0.5.7",
    "@genkit-ai/flow": "^0.5.7",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "graphology": "^0.25.4",
    "graphology-dag": "^0.4.1",
    "yaml": "^2.4.1",
    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "graphology-types": "^0.24.7",
    "npm-run-all": "^4.1.5",
    "tsup": "^8.0.2",
    "tsx": "^4.7.0",
    "typescript": "^4.9.0"
  },
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./lib/index.mjs",
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./functions": {
      "require": "./lib/functions.js",
      "import": "./lib/functions.mjs",
      "types": "./lib/functions.d.ts",
      "default": "./lib/functions.js"
    },
    "./functions/experimental": {
      "require": "./lib/experimental.js",
      "import": "./lib/experimental.mjs",
      "types": "./lib/experimental.d.ts",
      "default": "./lib/experimental.js"
    },
    "./auth": {
      "require": "./lib/auth.js",
      "import": "./lib/auth.mjs",
      "types": "./lib/auth.d.ts",
      "default": "./lib/auth.js"
    }
  },
  "typesVersions": {
    "*": {
      "functions": [
        "lib/functions"
      ],
      "functions/experimental": [
        "lib/functions"
      ],
      "auth": [
        "lib/auth"
      ]
    }
  }
}
