{
  "name": "@genkit-ai/plugin-firebase",
  "version": "0.0.5",
  "type": "commonjs",
  "scripts": {
    "check": "tsc",
    "compile": "tsup-node",
    "build:clean": "rm -rf ./lib",
    "build": "npm-run-all build:clean check compile",
    "build:watch": "tsup-node --watch",
    "lint": "eslint --config ../../.eslintrc.js --ext .ts src",
    "lint:fix": "npm run lint -- --fix --quiet"
  },
  "dependencies": {
    "@genkit-ai/common": "*",
    "firebase-admin": "^11.11.0",
    "firebase-functions": "^4.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "npm-run-all": "^4.1.5",
    "tsup": "^8.0.2",
    "tsx": "^4.7.0",
    "typescript": "^4.9.0"
  },
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./lib/index.mjs",
      "types": "./lib/index.d.ts"
    },
    "./functions": {
      "require": "./lib/functions.js",
      "import": "./lib/functions.mjs",
      "types": "./lib/functions.d.ts"
    },
    "./functions/experimental": {
      "require": "./lib/experimental.js",
      "import": "./lib/experimental.mjs",
      "types": "./lib/experimental.d.ts"
    },
    "./auth": {
      "require": "./lib/auth.js",
      "import": "./lib/auth.mjs",
      "types": "./lib/auth.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "functions": [
        "lib/functions"
      ],
      "functions/experimental": [
        "lib/functions"
      ],
      "auth": [
        "lib/auth"
      ]
    }
  }
}
