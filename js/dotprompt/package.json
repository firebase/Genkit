{
  "name": "@genkit-ai/dotprompt",
  "version": "0.0.6",
  "type": "commonjs",
  "scripts": {
    "check": "tsc",
    "compile": "tsup-node",
    "build:clean": "rm -rf ./lib",
    "build": "npm-run-all build:clean check compile",
    "build:watch": "tsup-node --watch",
    "test": "node --experimental-loader tsx --test tests/*_test.ts"
  },
  "dependencies": {
    "@genkit-ai/ai": "workspace:*",
    "@genkit-ai/core": "workspace:*",
    "front-matter": "^4.0.2",
    "handlebars": "^4.7.8",
    "node-fetch": "^3.3.2",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "npm-run-all": "^4.1.5",
    "tsup": "^8.0.2",
    "tsx": "^4.7.0",
    "typescript": "^4.9.0"
  },
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./lib/index.mjs",
      "types": "./lib/index.d.ts"
    }
  }
}
