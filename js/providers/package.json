{
  "name": "@genkit-ai/providers",
  "version": "0.0.4",
  "type": "commonjs",
  "main": "./lib/llms.mjs",
  "scripts": {
    "check": "tsc",
    "compile": "tsup-node",
    "build:clean": "rm -rf ./lib",
    "build": "npm-run-all build:clean check compile",
    "build:watch": "tsup-node --watch",
    "lint": "eslint --config ../.eslintrc.js --ext .ts src",
    "lint:fix": "npm run lint -- --fix --quiet"
  },
  "dependencies": {
    "@google-cloud/aiplatform": "^3.10.0",
    "@google-cloud/vertexai": "^0.3.1",
    "@genkit-ai/ai": "*",
    "@genkit-ai/common": "*",
    "@genkit-ai/flow": "*",
    "@google/generative-ai": "^0.2.0",
    "openai": "^4.24.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "tsup": "^8.0.2",
    "typescript": "^4.9.0"
  },
  "types": "lib/index.d.ts",
  "exports": {
    "./google-ai": {
      "types": "./lib/google-ai.d.ts",
      "require": "./lib/google-ai.js",
      "import": "./lib/google-ai.mjs"
    },
    "./google-vertexai": {
      "types": "./lib/google-vertexai.d.ts",
      "require": "./lib/google-vertexai.js",
      "import": "./lib/google-vertexai.mjs"
    },
    "./openai": {
      "types": "./lib/openai.d.ts",
      "require": "./lib/openai.js",
      "import": "./lib/openai.mjs"
    },
    "./ollama": {
      "types": "./lib/ollama.d.ts",
      "require": "./lib/ollama.js",
      "import": "./lib/ollama.mjs"
    }
  },
  "typesVersions": {
    "*": {
      "google-ai": [
        "lib/google-ai"
      ],
      "google-vertexai": [
        "lib/google-vertexai"
      ],
      "openai": [
        "lib/openai"
      ],
      "ollama": [
        "lib/ollama"
      ]
    }
  }
}
